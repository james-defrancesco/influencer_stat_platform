<p id="notice"><%= notice %></p>

<h1>Influencers</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Facebook</th>
      <th>Instagram</th>
      <th>Twitter</th>
      <th>Youtube</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @influencers.each do |influencer| %>
      <tr>
        <td><%= influencer.full_name %></td>
        <td><%= influencer.facebook %></td>
        <td><%= influencer.instagram %></td>
        <td><%= influencer.twitter %></td>
        <td><%= influencer.youtube %></td>
        <td><%= link_to 'Show', influencer %></td>
        <td><%= link_to 'Edit', edit_influencer_path(influencer) %></td>
        <td><%= link_to 'Destroy', influencer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Influencer', new_influencer_path %>
